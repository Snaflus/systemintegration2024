  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NodeJS: FileReaderTypescript Debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\FileReaderTypescript\\src\\Program.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "preLaunchTask": "tsc: build - tsconfig.json"
    },
    {
      "name": "C#: FileReaderDotnet Debug",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}\\FileReaderDotnet\\FileReaderDotnet.csproj"
    }
  ]
}
